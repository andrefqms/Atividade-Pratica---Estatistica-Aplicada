#questao 2
pois = 0
medias = 0
variances = 0
for (i in 1:1000) {
dist = rpois(100,10)
medias[i] = mean(dist)
variances[i] = var(dist)
}
hist(medias,col = 5)
hist(variances, col =6)
hist(medias,col = 5)
#questao 1
tam_populacao = 100
dados = rep(1, tam_populacao)
for (i in 1:25) {
dados[i] = 0
}
# amostras
k = 1000
n = 50
medias = rep(0, 1000)
variancias = rep(0, 1000)
for (j in 1:k) {
vetor = sample(dados, size=n, replace=FALSE)
medias[j] = mean(vetor)
variancias[j] = var(vetor)
}
media = mean(medias)
variancia = var(variancias)
media_variancias = mean(variancias)
#questao 1
