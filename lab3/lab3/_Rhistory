m = 10
med = rep(0, 1000)
varia = rep(0, 1000)
for (j in 1:k) {
vetor = sample(dados, size=m, replace=FALSE)
med[j] = mean(vetor)
varia[j] = var(vetor)
}
media10 = mean(med)
variancia10 = var(varia)
media_variancias10 = mean(varia)
hist(media10)
hist(media10)
