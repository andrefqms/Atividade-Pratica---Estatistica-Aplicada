#questao 3
expon = 0
for (i in 1:1000) {
di = rexp(100,5)
t = 1/mean(di)
expon[i] = t
}
hist(expon)
#questao 3
expon = 0
for (i in 1:1000) {
di = rexp(100,5)
t = 1/mean(di)
expon[i] = t
}
hist(expon)
#questao 3
expon = 0
for (i in 1:1000) {
di = rexp(100,5)
t = 1/mean(di)
expon[i] = t
}
hist(expon)
